state[anchor]}\n              onClose={toggleDrawer(anchor, false)}\n            >\n              {list(anchor)}\n            </Drawer>\n            <MenuIcon />\n          </IconButton>\n          <Typography className={classes.title} variant=\"h6\" noWrap>\n            RheinSchiff-App\n          </Typography>\n          <div className={classes.search} onClick={searchBoatHandler}>\n            <div className={classes.searchIcon}>\n              <SearchIcon />\n            </div>\n            <InputBase\n              placeholder=\"Searchâ€¦\"\n              classes={{\n                root: classes.inputRoot,\n                input: classes.inputInput,\n              }}\n              inputProps={{ \"aria-label\": \"search\" }}\n              onChange={(e) => setEnteredText(e.target.value)}\n            />\n          </div>\n        </Toolbar>\n      </AppBar>\n    </div>\n  );\n};\n\nexport default withStyles(useStyles, { withTheme: true })(SearchAppBarDrawer);\n","/Users/luis/DEV/Node/rheinschiff-app/frontend/src/shared/components/FormElements/NewBoatForm.jsx",["180"],"/Users/luis/DEV/Node/rheinschiff-app/frontend/src/shared/components/FormElements/Button.jsx",[],"/Users/luis/DEV/Node/rheinschiff-app/frontend/src/shared/components/UIElements/Card.jsx",[],"/Users/luis/DEV/Node/rheinschiff-app/frontend/src/boats/components/Schiff/Schiff.jsx",[],"/Users/luis/DEV/Node/rheinschiff-app/frontend/src/shared/components/UIElements/DeleteModal.jsx",[],"/Users/luis/DEV/Node/rheinschiff-app/frontend/src/shared/components/UIElements/Avatar.jsx",[],"/Users/luis/DEV/Node/rheinschiff-app/frontend/src/boats/pages/Login.jsx",[],{"ruleId":"181","replacedBy":"182"},{"ruleId":"183","replacedBy":"184"},{"ruleId":"181","replacedBy":"185"},{"ruleId":"183","replacedBy":"186"},{"ruleId":"181","replacedBy":"187"},{"ruleId":"183","replacedBy":"188"},{"ruleId":"189","severity":1,"message":"190","line":7,"column":8,"nodeType":"191","messageId":"192","endLine":7,"endColumn":14},{"ruleId":"189",