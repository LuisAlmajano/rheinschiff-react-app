b.filter((boat) => {\n      const regex = new RegExp(`${text}`, \"gi\");\n      return boat.name.match(regex);\n    });\n    setFiltered(filter);\n  };\n\n  const clearFilter = () => {\n    setFiltered(null);\n  };\n\n  // Pagination logic\n  // Change page\n  const paginate = (pageNum) => setCurrentPage(pageNum);\n\n  // Get current boats\n  const indexOfLastBoat = currentPage * boatsPerPage;\n  const indexOfFirstBoat = indexOfLastBoat - boatsPerPage;\n  const currentBoats = boats_db.slice(indexOfFirstBoat, indexOfLastBoat);\n\n  return (\n    <Fragment>\n      <SearchAppBarDrawer filterBoats={filterBoats} clearFilter={clearFilter} />\n      {filtered ? (\n        <Fragment>\n          <ScrollToTop />\n          <SchiffListe loading={loading} boats={filtered} />\n        </Fragment>\n      ) : (\n        <Fragment>\n          <ScrollToTop />\n          <SchiffListe loading={loading} boats={currentBoats} />\n          <Pagination boatsPerPage={boatsPerPage} totalBoats={boats_db.length} paginate={paginate} /> \n        </Fragment>\n      )}\n      <Footer />\n    </Fragment>\n  );\n};\n\n// @nice to have: Add transition to the filter\n// import { CSSTransition, TransitionGroup } from \"react-transition-group\";\n{\n  /* <Fragment>\n      <TransitionGroup>\n        {filtered !== null\n          ? filtered.map((contact) => (\n              <CSSTransition key={contact.id} timeout={500} classNames=\"item\">\n                <ContactItem contact={contact} />\n              </CSSTransition>\n            ))\n          : contacts.map((contact) => (\n              <CSSTransition key={contact.id} timeout={500} classNames=\"item\">\n                <ContactItem contact={contact} />\n              </CSSTransition>\n            ))}\n      </TransitionGroup>\n    </Fragment> */\n}\n\nexport default Boats;\n","/Users/luis/DEV/Node/rheinschiff-app/frontend/src/components/pages/Boat.jsx",[],"/Users/luis/DEV/Node/rheinschiff-app/frontend/src/components/pages/NewBoat.jsx",[],"/Users/lu